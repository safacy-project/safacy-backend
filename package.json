{
  "name": "safacy-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server-window": "set NODE_ENV=development && nodemon ./bin/www",
    "server-mac": "NODE_ENV=development nodemon ./bin/www",
